{
  "name": "whatsapp-bot-saas-backend",
  "version": "1.0.0",
  "description": "WhatsApp Bot SaaS Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "dev:simple": "ts-node --transpile-only src/server-simple.ts",
    "dev:db": "ts-node --transpile-only src/server.ts",
    "dev:quick": "ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:server": "node test-server.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "ts-node --project prisma/tsconfig.seed.json --transpile-only prisma/seed.ts",
    "kill-ports": "lsof -ti:3001,3000 | xargs kill -9"
  },
  "prisma": {
    "seed": "node prisma/seed-runner.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@prisma/client": "^5.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-webpmux": "^3.2.1",
    "openai": "^4.104.0",
    "pino": "^8.21.0",
    "pino-pretty": "^11.3.0",
    "puppeteer": "^24.24.1",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.7.4",
    "stripe": "^14.21.0",
    "whatsapp-web.js": "^1.34.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  }
}
